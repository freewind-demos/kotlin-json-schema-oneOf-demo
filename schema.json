{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "additionalProperties": false,
  "additionalItems": false,
  "properties": {
    "title": {
      "type": "string"
    },
    "from": {
      "type": "string"
    },
    "to": {
      "type": "string"
    },
    "address": {
      "type": "string"
    }
  },
  "oneOf": [
    {
      "required": [
        "title",
        "from",
        "to"
      ]
    },
    {
      "required": [
        "title",
        "address"
      ]
    }
  ]
}
